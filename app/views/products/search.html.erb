<div class="row small-up-1 medium-up-2 large-up-3">
  <% @result.each do |p| %>
    <div class="column">
      <div class="callout product-box">
        <span class="lead"><%= p['brand']['name'] %></span><br>
        <%= p['unbrandedName'] %><br>
        <% p['colors'].each do |a| %>
          <%= a['name'] %><br>
          <%= image_tag "#{a['swatchUrl']}" %><br>
        <% end %>
      </div>
    </div>
  <% end %>
</div>